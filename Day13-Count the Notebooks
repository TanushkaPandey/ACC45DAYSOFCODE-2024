
#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    int t;
    cin >> t;
    while(t--){

        double pulp ; 
        
        cin >> pulp ;

        
       
            
            cout << ceil((pulp * 1000) / 100)  << endl;
            
            // you can also do
            // cout << ceil(pulp * 10) << endl;
       
        
        
}
    return 0;
}
